{
  "name": "service-skeleton",
  "version": "1.0.0",
  "description": "",
  "main": "bootstrap.js",
  "scripts": {
    "postinstall": "direnv allow .",
    "test": "lab -t 1 test",
    "start": "nodemon --watch app --watch bootstrap.js bootstrap.js",
    "tdd": "nodemon --exec \"npm test\" --watch app --watch test --watch server.js",
    "linter": "eslint index.js app test || true",
    "migrate": "sequelize db:migrate --config src/core/db-config.js --migrations-path ${MIGRATIONS_PATH}",
    "migrate:undo": "sequelize db:migrate:undo --config src/core/db-config.js --migrations-path ${MIGRATIONS_PATH}",
    "generate:migration": "bash scripts/generate-migration.sh",
    "generate:seed": "bash scripts/generate-seed.sh",
    "generate:model": "bash scripts/generate-model.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samsystems/service-skeleton.git"
  },
