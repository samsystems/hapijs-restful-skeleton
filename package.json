  "bugs": {
    "url": "https://github.com/samsystems/service-skeleton/issues"
  },
  "homepage": "https://github.com/samsystems/service-skeleton#readme",
  "engines": {
    "node": ">=6.7.0"
  },
  "devDependencies": {
    "code": "^4.0.0",
    "eslint": "^3.5.0",
    "good-console": "^6.1.2",
    "lab": "^11.1.0",
    "nodemon": "^1.10.2",
    "sequelize-cli": "^2.4.0",
    "shrinkpack": "^0.17.0"
  },
  "dependencies": {
    "async": "^2.0.1",
    "blipp": "^2.3.0",
    "camelcase": "^3.0.0",
    "confidence": "^3.0.1",
    "glob": "^7.1.0",
    "glue": "^4.0.0",
    "good": "^7.0.2",
    "good-squeeze": "^5.0.0",
    "halacious": "^3.7.1",
    "hapi": "^15.1.1",
    "hapi-info": "^2.0.2",
    "hapi-swagger": "^7.2.0",
    "inert": "^4.0.2",
    "joi": "^9.1.0",
    "lodash": "^4.16.2",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "scooter": "^4.0.0",
    "sequelize": "^3.24.3",
    "vision": "^4.1.0"
  }
